<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <title>Anna Zaremba</title>
    <link href="{{ asset('css/style.css') }}" rel="stylesheet">
</head>
<body>
<div id="wrapper">
    <div id="logo">
        <a href="{{ path('homepage') }}">
            <h2>Anna Zaremba</h2>
        </a>
    </div>
    <div id="menu">
        <div id="search_top">
            <input type="text" size="30"/>
        </div>
        <ul>
            <li><a href="{{ path('homepage') }}">Strona główna</a></li>
            <li><a href="{{ path('omnie') }}">O mnie</a></li>
            <li><a href="{{ path('notatnik') }}">Notatnik</a></li>
            <li><a href="{{ path('kontakt') }}">Kontakt</a></li>
            <li><a href="{{ path('admin') }}">Moje konto</a></li>
        </ul>
    </div>
    <div id="header">
        <div id="title_head">
            <h2>Anna Zaremba</h2>
        </div>
    </div>
    <div id="container">
        {% block container %}
        {% endblock %}
        <div id="bar_right">
            <h2>KATEGORIE</h2>
            <div>
                <ul>
                    {% for kategoria in kategorie %}
                        <li>
                            <a href="{{ path('kategoriaid', { 'id': kategoria.id }) }}"><h3>{{ kategoria.nazwa|upper }}</h3></a>
                        </li>
                    {% endfor %}
                </ul>
            </div>
        </div>
    </div>
</div>
<div id="footer">
    <p>
    <div class="copyright">Copyright &copy; <a href="{{ path('homepage') }}">Anna Zaremba</a> 2017</div>
    </p>
</div>
</body>
</html>
