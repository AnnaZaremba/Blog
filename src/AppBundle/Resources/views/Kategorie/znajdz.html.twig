{% extends 'index.html.twig' %}
{% block container %}
    <div class="container">
        <div class="row">
            <h2>{{ kategoria.nazwa }}</h2>
            <div id="lista">
                <ul class="list-unstyled">
                    {% for wpis in wpisy %}
                        <li><a href="{{ path('wpisid', { 'id': wpis.id }) }}">{{ wpis.temat }}</a></li>
                    {% endfor %}
                </ul>
            </div>
        </div>
    </div>
{% endblock %}