{% extends 'index.html.twig' %}
{% block container %}
    <div class="row">
        <div class="col-lg-8" id="box_news">
            <h2>{{ kategoria.nazwa|upper }} </h2>
            <ul>
                {% for wpis in wpisy %}
                    <a href="{{ path('wpisid', { 'id': wpis.id }) }}"><h4>{{ wpis.temat }}</h4></a>
                {% endfor %}
            </ul>
        </div>
        {% block menu %}
            {% include('menu.html.twig') %}
        {% endblock %}
    </div>
{% endblock %}